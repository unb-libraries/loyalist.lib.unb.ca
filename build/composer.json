{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "php-http/discover": true,
      "phpstan/extension-installer": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of loyalist.lib.unb.ca",
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/loyalist.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/refs/tags/5.15.4.zip"
        },
        "extra": {
          "installer-name": "fontawesome"
        },
        "name": "fortawesome/font-awesome",
        "type": "drupal-library",
        "version": "5.15.4"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "3.3.0",
    "drupal/bootstrap5": "3.0.5",
    "drupal/core-composer-scaffold": "10.0.5",
    "drupal/core-recommended": "10.0.5",
    "drupal/core": "10.0.5",
    "drupal/easy_breadcrumb": "2.0.5",
    "drupal/entity_reference_revisions": "1.10",
    "drupal/fontawesome": "2.25",
    "drupal/health_check": "2.0.1",
    "drupal/paragraphs": "1.15",
    "drupal/pathauto": "1.11",
    "drupal/redis": "1.6",
    "drupal/samlauth": "3.8",
    "drupal/search_api": "1.28",
    "drupal/search_api_solr": "4.2.10",
    "drupal/twig_tweak": "3.2.1",
    "fortawesome/font-awesome": "5.15.4",
    "unb-libraries/drupal-builder": "^9",
    "unb-libraries/saml_features": "dev-9.x-1.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^9"
  },
  "type": "project"
}
