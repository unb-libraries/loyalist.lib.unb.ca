{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal component of loyalist.lib.unb.ca",
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/loyalist.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/refs/tags/5.15.4.zip"
        },
        "extra": {
          "installer-name": "fontawesome"
        },
        "name": "fortawesome/font-awesome",
        "type": "drupal-library",
        "version": "5.15.4"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/addtoany": "^2",
    "drupal/admin_toolbar": "^3",
    "drupal/better_exposed_filters": "^6",
    "drupal/blog": "^3",
    "drupal/bootstrap5": "^3",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/core": "^10",
    "drupal/easy_breadcrumb": "^2",
    "drupal/entity_reference_revisions": "^1",
    "drupal/field_group": "^3",
    "drupal/fontawesome": "^2",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^2",
    "drupal/migrate_plus": "^6",
    "drupal/migrate_source_csv": "^3",
    "drupal/paragraphs": "^1",
    "drupal/pathauto": "^1",
    "drupal/redis": "^1",
    "drupal/samlauth": "^3",
    "drupal/search_api": "^1",
    "drupal/search_api_solr": "^4",
    "drupal/twig_tweak": "^3",
    "fortawesome/font-awesome": "5.15.4",
    "unb-libraries/composer-drupal-scaffold": "^10"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}
