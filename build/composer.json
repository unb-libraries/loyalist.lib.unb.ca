{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of loyalist.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/loyalist.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/refs/tags/5.15.4.zip"
        },
        "extra": {
          "installer-name": "fontawesome"
        },
        "name": "fortawesome/font-awesome",
        "type": "drupal-library",
        "version": "5.15.4"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "3.1.1",
    "drupal/bootstrap5": "2.0.0",
    "drupal/core": "9.4.8",
    "drupal/easy_breadcrumb": "2.0.3",
    "drupal/entity_reference_revisions": "1.9",
    "drupal/fontawesome": "2.23",
    "drupal/google_analytics": "2.5",
    "drupal/health_check": "1.3",
    "drupal/paragraphs": "1.14",
    "drupal/pathauto": "1.11",
    "drupal/redis": "1.6",
    "drupal/samlauth": "3.6",
    "drupal/search_api": "1.25",
    "drupal/search_api_solr": "4.2.8",
    "drupal/twig_tweak": "3.1.3",
    "fortawesome/font-awesome": "5.15.4",
    "unb-libraries/drupal-builder": "^9",
    "unb-libraries/saml_features": "dev-9.x-1.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^9"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
